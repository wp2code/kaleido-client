{
  "name": "kaleido-client",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "wp2code",
  "productName": "kaleido",
  "homepage": "https://github.com/wp2code",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false --experimentalDecorators true --emitDecoratorMetadata true",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch --mode development ",
    "build": "electron-vite build --mode production",
    "postinstall": "electron-builder install-app-deps",
    "createSigned": "electron-builder create-self-signed-cert --publisher kaleido",
    "build:win": "npm run build && electron-builder --config  -c.extraMetadata.version=1.0.2 --win --x64 --publish never",
    "build:mac": "npm run build && electron-builder --mac --config --publish never",
    "build:linux": "npm run build && electron-builder --linux --config --publish never",
    "build:help": "electron-builder --help"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.7",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-vue": "^0.1.3",
    "@codemirror/state": "^6.4.0",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@electron-toolkit/utils": "^1.0.2",
    "@element-plus/icons-svg": "^2.1.0",
    "@element-plus/icons-vue": "^2.1.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "codemirror": "^6.0.1",
    "electron-store": "8.2.0",
    "electron-updater": "^5.3.0",
    "element-plus": "^2.3.8",
    "form-data": "^4.0.0",
    "howler": "^2.2.3",
    "https": "^1.0.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "pinia": "^2.1.4",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "screenfull": "^6.0.2",
    "url": "^0.11.1",
    "vue-axios": "^3.5.2",
    "vue-codemirror": "^6.1.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@iconify-json/ep": "^1.1.10",
    "@rushstack/eslint-patch": "^1.3.2",
    "@swc/core": "^1.3.100",
    "@types/codemirror": "^5.60.7",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.19",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@unocss/preset-attributify": "^0.53.5",
    "@unocss/preset-icons": "^0.53.5",
    "@unocss/preset-uno": "^0.53.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "electron": "31.7.0",
    "electron-builder": "^25.1.8",
    "electron-log": "^5.2.0",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^1.0.25",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.27",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.64.1",
    "ts-node": "10.7.0",
    "typescript": "^5.1.6",
    "unocss": "^0.53.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-element-plus": "^0.7.2",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.4"
  }
}
